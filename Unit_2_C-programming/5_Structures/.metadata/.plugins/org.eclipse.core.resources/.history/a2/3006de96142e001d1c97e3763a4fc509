#include <stdio.h>
#include <stdlib.h>

int main(void) {

	char *ptr =(char*)malloc(10);
	if(NULL== ptr)
	{
		printf("ddd");
				return ;
	}
	else
	{
		free(ptr);
	}
	return;
}
