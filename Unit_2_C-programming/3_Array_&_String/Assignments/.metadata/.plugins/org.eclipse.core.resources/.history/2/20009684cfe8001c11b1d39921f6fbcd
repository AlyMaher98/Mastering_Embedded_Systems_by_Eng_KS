#include <stdio.h>

int main(void)
{
	int x;
	int array[10];
	printf("Enter no of elements : ");
	fflush(stdout);
	scanf("%d",&x);

	for (int i=0;i<x;i++)
	{
		scanf("%d",&array[i]);
	}
}
