#include <stdio.h>

int main(void)
{
	int x; // number of elements
	int z; // the element we want to search
	int n;
	int array[10];
	printf("Enter no of elements : ");
	fflush(stdout);
	scanf("%d",&x);
	for (int i=0;i<x;i++)
	{
		scanf("%d",&array[i]);
	}
	printf("Enter the element to be searched :  ");
	fflush(stdout);
	scanf("%d",&z);

	printf("Number founded at the location = %d ",n);

}
