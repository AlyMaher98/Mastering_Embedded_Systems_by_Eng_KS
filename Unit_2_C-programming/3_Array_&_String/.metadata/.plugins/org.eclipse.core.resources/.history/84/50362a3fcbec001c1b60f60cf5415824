#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	int array_rows;
	int array_cloumns;
	int x;
	printf("Enter rows and column of matrix : ");
	fflush(stdout);
	scanf("%d %d",&array_rows,&array_cloumns);
	printf("Enter elements of matrix : ");
for(int i=0;i<array_rows;i++)
{
	for(int j=0;j<array_cloumns;j++)
	{
		printf("Enter elements of matrix: ");
		printf("Enter elements of a%d%d: ",i-1,j-1);
		fflush(stdout);
		scanf("%d",&x);
	}
}

}
