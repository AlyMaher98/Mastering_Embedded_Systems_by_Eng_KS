#include <stdio.h>
#include <stdlib.h>

void power (int a,int b)
{
	int counter=1;
	if(counter==3)
	{
		return;
	}
	else(counter<3)
	{
		printf("%d",5*power(a,counter));
	}
}
int main(void) {
	int x,y;
	printf("enter base number: %d",x);
	fflush(stdout);
	scanf("%d",&x);
	printf("Enter power number (positive integer: %d",y);
	fflush(stdout);
	scanf("%d",&y);
	power(x,y);

}
