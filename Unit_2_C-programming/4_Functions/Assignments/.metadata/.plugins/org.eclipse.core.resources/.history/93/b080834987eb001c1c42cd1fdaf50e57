#include <stdio.h>
int prime_numbers_print(int x,int y)
{
	printf("Prime numbers between %d and %d are: ");
	int temp1,temp2;
	for(int n=0;n<;n++)
		{
		temp1=6*n+1;
		temp2=6*n-1;
			if (temp1>x&temp1<y)
			{
				printf("%d",temp1);
			}
			if (temp1>x&temp1<y)
				{
					printf("%d",temp1);
				}

		}

}
int main(void)
{
	int x,y;
	printf("Enter the two numbers\n");
	fflush(stdout);
	scanf("%d &d",&x,&y);
	prime_numbers_print(x,y);


}
